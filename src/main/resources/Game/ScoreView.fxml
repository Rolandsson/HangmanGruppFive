<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="440.0" prefWidth="681.0" style="-fx-background-color: #bdfaa1;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.ScoreViewControl">
   <children>
      <Button fx:id="nextButton" layoutX="314.0" layoutY="282.0" mnemonicParsing="false" onAction="#switchGameScene" style="-fx-background-color: lightblue;" text="Next">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </Button>
      <Button fx:id="newGameButton" layoutX="567.0" layoutY="382.0" mnemonicParsing="false" style="-fx-background-color: #ce77d1;" text="New Game" textFill="#151415">
         <font>
            <Font name="Comic Sans MS" size="13.0" />
         </font>
      </Button>
      <Label fx:id="player1Score" layoutX="272.0" layoutY="87.0" text="Player 1 score:" textFill="#f797c7">
         <font>
            <Font name="Comic Sans MS" size="22.0" />
         </font>
      </Label>
      <Label fx:id="player2Score" layoutX="272.0" layoutY="129.0" text="Player 2 score:" textFill="#6db8e4">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Text fill="#211971" layoutX="273.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score Board">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
